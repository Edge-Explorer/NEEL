{
    "cli": {
        "version": ">= 12.6.3",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_NO_DOCTOR": "1",
                "EAS_NO_DOCTOR": "1",
                "EXPO_DEBUG": "1"
            }
        }
    },
    "submit": {
        "production": {}
    }
}